{
  "extends": [
    "@rocketseat/eslint-config/next",
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "camelcase": "off",
    "prettier/prettier": [
      "error", {
        "trailingComma": "none",
        "endOfLine": "auto"
      }
    ]
  }
}